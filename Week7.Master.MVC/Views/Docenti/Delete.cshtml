@model DocenteViewModel


<form method="post">
    <p>Sei sicuro di voler eliminare il docente ?</p>

    <div class="form-group">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" class="form-control" readonly />
        <span asp-validation-for="Nome" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Cognome" class="form-label"></label>
        <input asp-for="Cognome" class="form-control" readonly />
        <span asp-validation-for="Cognome" class="text-danger"></span>
    </div>

    <input type="submit" value="Conferma" class="btn btn-danger" />
    <a class="btn btn-primary" asp-action="Index" role="button">Back</a>
</form>

